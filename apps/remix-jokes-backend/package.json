{
  "private": true,
  "name": "remix-jokes-backend",
  "version": "0.1.0",
  "license": "MIT",
  "main": "server.ts",
  "engines": {
    "node": ">=16.14.0 <17.0.0"
  },
  "scripts": {
    "dev": "prisma migrate dev && nodemon --exec \"ts-node --files ./src/server.ts\" -e .ts",
    "build": "prisma migrate deploy && tsc",
    "prestart": "tsc",
    "start": "node dist/server.js",
    "quit:dev": "kill -9 $(sudo lsof -i :5001 | grep node | awk '{print $2}')",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:status": "prisma migrate status",
    "generate": "prisma generate",
    "seed": "ts-node prisma/seed.ts",
    "lint": "TIMING=1 eslint src/'**/*.{ts,tsx}' --fix",
    "format:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\"",
    "format:fix": "prettier --write .",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf .cache && rm -rf package-lock.json && rm -rf yarn.lock",
    "typecheck": "tsc -b"
  },
  "jest": {
    "preset": "scripts/jest/node"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-flash": "^0.0.2",
    "express-session": "^1.17.2",
    "morgan": "^1.10.0",
    "redis": "^4.0.4",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.0.0",
    "jsonwebtoken": "^8.5.1",
    "lint-staged": "^12.3.7",
    "nodemon": "^2.0.15",
    "prettier": "2.6.1",
    "pretty-quick": "^3.1.3",
    "prisma": "^3.11.1",
    "ts-node": "^10.7.0"
  }
}
