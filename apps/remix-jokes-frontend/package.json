{
  "name": "remix-jokes-frontend",
  "private": true,
  "version": "0.1.0",
  "sideEffects": false,
  "engines": {
    "node": ">=16.14.0 <17.0.0"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "tailwindcss -m -i ./app/styles/tailwind-input.css -o ./app/styles/tailwind-output.css",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "tailwindcss -w -i ./app/styles/tailwind-input.css -o ./app/styles/tailwind-output.css",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node ./build/index.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "clean": "rm -rf node_modules && rm -rf dist && rm -rf .cache && rm -rf public/build && rm -rf package-lock.json && rm -rf yarn.lock",
    "format": "prettier --write .",
    "typecheck": "tsc -b",
    "validate": "run-p lint typecheck"
  },
  "dependencies": {
    "@prisma/client": "^3.11.1",
    "@remix-run/react": "*",
    "@remix-run/serve": "*",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^8.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "*"
  },
  "devDependencies": {
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@types/bcryptjs": "^2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/react": "^17.0.43",
    "@types/react-dom": "^17.0.14",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "prettier": "^2.6.1",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.23",
    "typescript": "^4.6.3"
  }
}
